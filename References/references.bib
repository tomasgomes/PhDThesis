	author = {Tang, Fuchou and Barbacioru, Catalin and Wang, Yangzhou and Nordman, Ellen and Lee, Clarence and Xu, Nanlan and Wang, Xiaohui and Bodeau, John and Tuch, Brian B. and Siddiqui, Asim and Lao, Kaiqin and Surani, M. Azim},
	month = may,
	year = {2009},
	pages = {377--382},
	file = {Full Text PDF:/Users/tpcg/Zotero/storage/B7B2GY4I/Tang et al. - 2009 - mRNA-Seq whole-transcriptome analysis of a single .pdf:application/pdf;Snapshot:/Users/tpcg/Zotero/storage/ML7WYWLU/nmeth.html:text/html}
}

